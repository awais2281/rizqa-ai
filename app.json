{
  "expo": {
    "name": "Rizqa AI",
    "slug": "rizqa-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*",
      "models/**",
      "assets/**"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rizqaai.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.rizqaai.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "NEVER",
      "fallbackToCacheTimeout": 0,
      "url": ""
    },
    "extra": {
      "eas": {
        "projectId": "7eebecfa-d4ee-4b31-ba3f-a2687706c931"
      }
    },
    "plugins": [
      "expo-audio"
    ],
    "owner": "mufctt"
  }
}
